       <string notr="true">/* Style for QDateEdit */
QDateEdit {
    border-radius: 10px;
    background-color: rgb(27, 26, 56);
    padding-left: 7px;
    color: white;
    font: 10pt &quot;Poppins SemiBold&quot;;
}

/* Style for QDateEdit's drop-down button */
QDateEdit::drop-down {
	border: 10px;
    width: 30px;
    subcontrol-origin: padding;
    subcontrol-position: top right;
    /* border-left: 1px solid white; Optional: add a border to separate the button */
}

QDateEdit::down-arrow {
    image: url(:/svg_icons/caret-down.svg); /* Use your custom icon here */
    width: 15px;
    height: 15px;
}

/* Style for the calendar popup */
QCalendarWidget {
    background-color: rgb(27, 26, 56);
    color: white;
    border-radius: 10px;
    font: 8pt &quot;Poppins&quot;;
}

QCalendarWidget QToolButton {
    background-color: rgb(27, 26, 56);
    color: white;
    border: none;
	padding-right: 15px;
	padding-left: 15px;
    font: 8pt &quot;Poppins&quot;;
}

QCalendarWidget QAbstractItemView:enabled {
    color: white;
    background-color: rgb(27, 26, 56);
    selection-background-color: rgb(40, 39, 74);
    selection-color: white;
    border: none;
}

QCalendarWidget QWidget#qt_calendar_navigationbar {
    background-color: rgb(27, 26, 56);  /* Match your main bg */
    color: white;
}

QCalendarWidget QToolButton#qt_calendar_prevmonth,
QCalendarWidget QToolButton#qt_calendar_nextmonth {
    qproperty-icon: none;  /* Remove default arrows if needed */
    color: white;
    background: transparent;
}

QCalendarWidget QToolButton#qt_calendar_nextmonth {
    image: url(:/svg_icons/square-right.svg);
    width: 15px;
    height: 15px;
}

QCalendarWidget QToolButton#qt_calendar_prevmonth {
    image: url(:/svg_icons/square-left.svg);
    width: 15px;
    height: 15px;
}

QCalendarWidget QMenu {
    background-color: rgb(27, 26, 56);
    color: white;
    border: 1px solid white;
}

/* Weekday names text */
QCalendarWidget QAbstractItemView:disabled {
    color: white;  /* Weekday names color */
    font: 8pt &quot;Poppins SemiBold&quot;;  /* Make slightly bolder than dates */
    background-color: transparent;
}

QCalendarWidget QTableView {
    alternate-background-color: rgb(20, 31, 69); /* Header bg */
}

QCalendarWidget QTableView::item {
    color: white; /* Weekday text color */
    background-color: rgb(20, 31, 69); /* Cell bg */
    font: 8pt &quot;Poppins SemiBold&quot;;
}

