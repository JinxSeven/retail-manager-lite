        <string notr="true">/* QTableWidget Dark Mode - Compact */
QTableWidget {
    border-radius: 8px;
    gridline-color: #44475a; /* Subtle grid lines */
    color: #F8F8F2; /* Light text color */
    font: 9pt &quot;Poppins&quot;; /* Smaller font */
    background-color: #1E1E1E; /* Dark background */
    border: 1px solid #1E1E1E; /* Border blends with background */
}

/* QTableWidget Items */
QTableWidget::item {
    border: none;
    padding: 2px 5px; /* Reduce padding for compact view */
}

/* Header Style - Dark Mode */
QHeaderView::section {
    background-color: #2E2E2E; /* Dark header */
    padding: 3px 6px; /* Smaller padding */
    border: none;
    border-radius: 8px;
